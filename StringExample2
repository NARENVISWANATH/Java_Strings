/**
 *
 * @author NAREN
 */
public class StringExample7 {
    public static void main(String args[])
    {
        String s1 = "RajaRamMohanRoy";
        System.out.println(s1.toUpperCase());//RAJARAMMOHANROY
        System.out.println(s1.toLowerCase());//rajarammohanroy
        System.out.println(s1.startsWith("Raja"));//true
        System.out.println(s1.contains("Moha"));//true
        System.out.println(s1.endsWith("Roy"));//true
        System.out.println(s1.charAt(7));//M
System.out.println(s1.charAt(7));//M
        System.out.println(s1.charAt(10));//a
        System.out.println(s1.indexOf('M'));//7
        System.out.println(s1.substring(7,12));//7 is inclusive and 12 is exclusive
        System.out.println(s1.length());//15
        
        
    }
    
}

