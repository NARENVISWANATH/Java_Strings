 */
public class StringExample2 {
    public static void main(String args[])
    {
        String s = "Rama";
        String s1 = "rama";
        if(s.equals(s1)== true)
        {
            System.out.println("Strings are equal");
        }
        else
        {
            System.out.println("Strings are not equal");
        }
    }
    
}

